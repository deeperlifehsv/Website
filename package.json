{
  "name": "deeperlife-huntsville",
  "version": "1.0.0",
  "description": "Deeper Life Bible Church Huntsville - Website Redesign",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:studio\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:studio": "cd apps/studio && npm run dev",
    "build": "npm run build:web && npm run build:studio",
    "build:web": "cd apps/web && npm run build",
    "build:studio": "cd apps/studio && npm run build",
    "lint": "cd apps/web && npm run lint",
    "clean": "rm -rf node_modules apps/*/node_modules apps/*/.next"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
